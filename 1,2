public class Main {
   int x=5;
   void sum()
   {
       x=x+5;
       System.out.println(x);
   }
   public static void main(String[] args) {

       student obj=new student();
       obj.name= "Munna";
       obj.age= 20;
       obj.cgpa= 3.5F;
       obj.display ();
   }
}

public class student {
   String name;
   int age;
   float cgpa;
   void display()
   {

       System.out.println(name);
       System.out.println(age);
       System.out.println(cgpa);

   }
}

